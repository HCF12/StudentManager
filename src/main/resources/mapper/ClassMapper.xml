<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace= "cn.com.mapper.ClassMapper" >
    <select id="getClassByGrade" resultType="cn.com.pojo.StudentClass">
        select class_id,class_name from class where grade_id = #{gradeId}
    </select>
</mapper>